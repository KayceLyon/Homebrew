      <div class="grid-x grid-margin-x small-up-2 medium-up-3">
        <div class="cell">
          <div class="card">
            <div class="card-section">
              <h4>Race</h4>
              <p><a href="/races">Add Homebrew Race</a></p>
            </div>
          </div>
        </div>
        <div class="cell">
          <div class="card">
            <div class="card-section">
              <h4>Spell</h4>
              <p><a href="/spells">Add Homebrew Spell</a></p>
            </div>
          </div>
        </div>
        <div class="cell">
          <div class="card">
            <div class="card-section">
              <h4>Spell</h4>
              <p><a href="/subclasses">Add Homebrew Sub-Class</a></p>
            </div>
          </div>
        </div>
      </div>
        <div class="grid-x grid-margin-x small-up-2 medium-up-3">
        <div class="cell">
          <div class="card">
            <div class="card-section">
              <h4>Weapon</h4>
              <p><a href="/weapons">Add Homebrew Weapon</a></p>
            </div>
          </div>
        </div>
      </div>
    </div>
